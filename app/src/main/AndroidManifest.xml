<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.zucc.ccm31501396.mdays">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <receiver android:name=".AlarmReceiver">
            <intent-filter>
                <action android:name="intent_alarm_log"/>
            </intent-filter>
        </receiver>
        <activity android:name=".Register" />
        <activity
            android:name=".UseAcitvity"
            android:label="@string/title_activity_use_acitvity" />
        <activity android:name=".AddAccountActivity" />
        <activity android:name=".SearchAccount" />
        <activity android:name=".SingleAccount" />
        <activity android:name=".SingleSchedule" />
        <activity android:name=".SearchSchedule" />
        <activity android:name=".AddSchedule"></activity>
    </application>

</manifest>